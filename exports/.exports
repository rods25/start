alias exports="e $DOTFILES_EXPORTS_PATH/.exports"

export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion

# -- OS VARIABLES - Paths
export START="$HOME/Documents/start"
export SVNPATH="$START/svn"
export SVNQAPATH="$START/svn-qa"
export TCLPATH="$START/templatecache-local"
export GITPATH="$START/git"
export NGINX_DIR="/etc/nginx"

export PATH=$PATH:$HOME/.npm_global_modules/bin
npm config set prefix=$HOME/node